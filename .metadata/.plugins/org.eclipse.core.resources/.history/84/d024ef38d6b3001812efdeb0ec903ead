
public interface Moverse{
	
	public static void moverseAlNorte(Nave nave){
		nave.avanzarAlNorte();
	}
	
	public static void moverseAlSur(Nave nave){
		nave.avanzarAlSur();
	}
	
	public static void moverseAlEste(Nave nave){
		nave.avanzarAlEste();
	}
	
	public static void moverseAlOeste(Nave nave){
		nave.avanzarAlOeste();
	}
	
}
